import java.util.LinkedList;
public class Main
{
    static void meth()
    {
       LinkedList<Object> ll=new LinkedList<Object>();
       
       ll.add(10);      //insertion order is maintained
       ll.add(20);          //heterogeneous elements allowed
       ll.add(null);        //null value allowed
       ll.add("dryden");    //duplicates allowed
       ll.add('d');         //default size is 0
       ll.add(10);          //not synchronized
       ll.add(50);          //size increases by double
       
       ll.add(1,40);        //add the elements in middle
       
       System.out.println(ll);
       System.out.println(ll.size());
       System.out.println(ll.isEmpty());
       System.out.println(ll.contains("dryden"));
       System.out.println(ll.get(4));
    }
	public static void main(String[] args) 
	{
	    meth();
		
	}
}
