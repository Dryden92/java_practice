public class Main
{
	public static void main(String[] args) 
	{
	    int a=0;
	    int b=1;
	    int n=4;
	    int sum=0;
	    
	    for(int i=0; i<=n;i++)
	    {
	        sum=sum+a;
	        
	        int c=a+b;
	        a=b;
	        b=c;
	    }
	    System.out.println(sum);
	    //0+1+1+2+3+5+8
	}
}
