public class Main
{
    void meth()
    {
        int n=7;
        
        int arr[]=new int[35];
        
        int i=0;
        
       while(n>0)
       {
           arr[i]=n%2;
           
           n=n/2;
           
           i++;
       }
        
        for(int j=i-1;j>=0;j--)
        {
            System.out.print(arr[j]);
        }
        
    }
	public static void main(String[] args)
	{
		Main obj=new Main();
		obj.meth();
	}
}
