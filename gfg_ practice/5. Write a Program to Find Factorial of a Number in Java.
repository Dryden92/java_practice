public class Main
{
    void meth()
    {
        //5. Write a Program to Find Factorial of a Number in Java.
        
        int n=6;
        
        int sum=1;
        
        for(int i=n;i>0;i--)
        {
            sum=sum*i;
        }
        System.out.println(sum);
    }
    int meth2(int n)
    {
        if(n==0)
        {
            return 1;
        }
     
        return n*meth2(n-1);
    }
	public static void main(String[] args) 
	{
	//	new Main().meth();
	    Main obj=new Main();
		
		int s=obj.meth2(5);
		System.out.println(s);
	}
}
